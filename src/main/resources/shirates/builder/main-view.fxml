<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<VBox fx:id="rootNode" alignment="CENTER" prefHeight="300.0" prefWidth="400.0" style="-fx-font-size: 14px;"
      xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="shirates.builder.MainController">
    <children>
        <Button fx:id="screenBuilderButton" mnemonicParsing="false" prefWidth="200.0" text="Screen Builder">
            <VBox.margin>
                <Insets bottom="16.0" left="16.0" right="16.0" top="16.0"/>
            </VBox.margin>
        </Button>
        <Button fx:id="templateCodeGeneratorButton" mnemonicParsing="false" prefWidth="200.0"
                text="Template Code Generator">
            <VBox.margin>
                <Insets bottom="16.0" left="16.0" right="16.0" top="16.0"/>
            </VBox.margin>
        </Button>
    </children>
</VBox>
